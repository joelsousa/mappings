	<xsd:complexType name="CT_TIN">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type TIN</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type TIN </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TIN" type="xsd:integer"/>
			<xsd:element name="IsTINGermanApprovalNumber" type="ST_ConfirmationCode"/>
