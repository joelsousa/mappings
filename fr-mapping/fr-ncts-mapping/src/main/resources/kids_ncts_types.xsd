	<!-- ###############################################################  -->
	<!-- simple types -->
	<xsd:simpleType name="ST_KindOfError">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="ERR">
				<xsd:annotation>
					<xsd:documentation>Error</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WRG">
				<xsd:annotation>
					<xsd:documentation>Warning</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="INF">
				<xsd:annotation>
					<xsd:documentation>Information</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_CustomsOffice">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="8"/>
			<xsd:maxLength value="8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_Currency">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="3"/>
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_ConfirmationCode">
		<xsd:restriction base="xsd:boolean"/>
	</xsd:simpleType>
	<xsd:simpleType name="ST_CountryISOCode">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="2"/>
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_Date">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{8}">
				<xsd:annotation>
					<xsd:documentation>
						format:		YYYYMMDD 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_Time">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="6"/>
			<xsd:maxLength value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_DateTime">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{12}">
				<xsd:annotation>
					<xsd:documentation>
						format:		YYYYMMDDHHMM
					</xsd:documentation>
				</xsd:annotation>
			</xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_DateFormatted">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{4}[\-][0-9]{2}[\-][0-9]{2}">
				<xsd:annotation>
					<xsd:documentation>
						format:		YYYY-MM-DD 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_DateTimeFormatted">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{4}[\-][0-9]{2}[\-][0-9]{2}(T)[0-9]{2}[:][0-9]{2}[:][0-9]{2}([+\-][0-9]{2}[:][0-9]{2}|(Z))">
				<xsd:annotation>
					<xsd:documentation>
						format:		YYYY-MM-DDTHH:MM:SS(+HH:MM|Z)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_Coefficient">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<xsd:simpleType name="ST_PartValue">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<xsd:simpleType name="ST_TimeFormatted">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{2}[:][0-9]{2}([:][0-9]{2}([.][0-9]{3}))">
				<xsd:annotation>
					<xsd:documentation>
						format:		HH:MM[:SS[.sss]] 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_Indicator">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Indicator yes / no</xsd:documentation>
			<xsd:documentation xml:lang="de">Indikator Deutsche Codeliste A0027 ja/nein</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">no</xsd:documentation>
					<xsd:documentation xml:lang="de">nein</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="1">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">yes</xsd:documentation>
					<xsd:documentation xml:lang="de">ja</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
