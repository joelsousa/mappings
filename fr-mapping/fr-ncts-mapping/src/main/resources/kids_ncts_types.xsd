	<xsd:complexType name="CT_TINType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<!-- #20081007 KC CT_TIN no longer exists
			<xsd:element name="TIN" type="CT_TIN"/>
			-->
			<xsd:element name="TIN" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>TIN/EORI</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsTINGermanApprovalNumber" type="ST_ConfirmationCode" minOccurs="0"/>
			<xsd:element name="CustomerIdentifier" type="xsd:string" minOccurs="0"/>
			<xsd:element name="BO" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="IdentificationType" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>0=Addressdata, 2=EORI-TIN
or KCX-Code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
