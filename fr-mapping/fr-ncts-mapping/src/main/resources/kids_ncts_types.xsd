<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!-- complex types for NCTS -->
	<xsd:complexType name="CT_Amount">
		<xsd:sequence>
			<xsd:element name="Amount" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
			<xsd:element name="LocalAmount" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="LocalCurrency" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_EnRouteEvent">
		<xsd:sequence>
			<xsd:element name="Place" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CountryCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="TransShipment" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="NewTransportId" minOccurs="0"/>
						<xsd:element name="NewTransportCountry" type="ST_CountryISOCode" minOccurs="0"/>
						<xsd:element name="EndorsementDate" type="ST_Date" minOccurs="0"/>
						<xsd:element name="EndorsementAuthority" minOccurs="0"/>
						<xsd:element name="EndorsementPlace" minOccurs="0"/>
						<xsd:element name="EndorsementCountry" minOccurs="0"/>
						<xsd:element name="ContainerNumber" minOccurs="0" maxOccurs="99"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="TotalNumberOfSeals" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="SealsIdentity" minOccurs="0" maxOccurs="9999"/>
			<xsd:element name="Incident" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="IncidentFlag" type="xsd:int" minOccurs="0"/>
						<xsd:element name="IncidentInfo" minOccurs="0"/>
						<xsd:element name="EndorsementDate" type="ST_Date" minOccurs="0"/>
						<xsd:element name="EndorsementAuthority" minOccurs="0"/>
						<xsd:element name="EndorsementPlace" minOccurs="0"/>
						<xsd:element name="EndorsementCountry" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="AlreadyInNCTS" type="xsd:int" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Guarantee">
		<xsd:sequence>
			<xsd:element name="TypeOfGuarantee" minOccurs="0"/>
			<xsd:element name="Reference" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="GRN" type="xsd:string" minOccurs="0"/>
						<xsd:element name="AccessCode" type="xsd:string" minOccurs="0"/>
						<xsd:element name="OtherNumber" type="xsd:string" minOccurs="0"/>
						<xsd:element name="IssuingOffice" type="xsd:string" minOccurs="0"/>
						<xsd:element name="OwnerTIN" type="xsd:string" minOccurs="0"/>
						<xsd:element name="PriceGroup" type="xsd:string" minOccurs="0"/>
						<xsd:element name="NotValidForEC" type="xsd:string" minOccurs="0"/>
						<xsd:element name="NotValidForCountry" type="xsd:string" minOccurs="0" maxOccurs="99"/>
						<xsd:element name="Amounts" type="CT_Amount" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ResultsOfControl">
		<xsd:sequence>
			<xsd:element name="Description" minOccurs="0"/>
			<xsd:element name="ControlIndicator" minOccurs="0"/>
			<xsd:element name="AttributePointer" minOccurs="0"/>
			<xsd:element name="CorrectedValue" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_UnloadingRemark">
		<xsd:sequence>
			<xsd:element name="StateOfSealsOk" type="xsd:int" minOccurs="0"/>
			<xsd:element name="Remarks" minOccurs="0"/>
			<xsd:element name="Conform" type="xsd:int" minOccurs="0"/>
			<xsd:element name="UnloadingCompletion" type="xsd:int" minOccurs="0"/>
			<xsd:element name="UnloadingDate" type="ST_Date" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_WriteOffSumA">
		<xsd:sequence>
			<xsd:element name="Identification" type="xsd:string" minOccurs="0"/>
			<!-- Art der Identifikation -->
			<xsd:element name="Reference" type="xsd:string" minOccurs="0"/>
			<!-- Bezugsnummer -->
			<xsd:element name="FlightNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="WriteOffData" minOccurs="0" maxOccurs="9999">
				<!-- Position SumA -->
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="NumberOfPieces" type="xsd:string" minOccurs="0"/>
						<!-- Stückzahl -->
						<xsd:element name="RegNumber" type="xsd:string" minOccurs="0"/>
						<!-- Registriernummer -->
						<xsd:element name="ItemNumber" type="xsd:integer" minOccurs="0"/>
						<!-- Positionsnummer -->
						<xsd:element name="AWBNumber" type="xsd:string" minOccurs="0"/>
						<!-- AWB -->
						<xsd:element name="Custodian" type="xsd:string" minOccurs="0"/>
						<!-- Verwahrer/TIN -->
						<xsd:element name="CustomerID" minOccurs="0"/>
						<xsd:element name="SpoArt" type="xsd:string" minOccurs="0"/>
						<!-- Spezifischer Ordnungsbegriff/Art -->
						<xsd:element name="SpO" type="xsd:string" minOccurs="0"/>
						<!-- Spezifischer Ordnungsbegriff/Nummer -->
						<xsd:element name="ATLASAlignment" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
		<!-- Beendigung SumA -->
	</xsd:complexType>
	<!--  EI20120206 -->
	<xsd:complexType name="CT_ManifestCompletion">
		<xsd:annotation>
			<xsd:documentation>WriteOffSumA</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Identification" type="xsd:string" minOccurs="0"/>
			<!-- Art der Identifikation -->
			<xsd:element name="ReferenceNumber" type="xsd:string" minOccurs="0"/>
			<!-- Bezugsnummer -->
			<xsd:element name="FlightNumber" type="xsd:string" minOccurs="0"/>
			<!-- Position SumA -->
			<xsd:element name="CompletionItem" type="CT_ManifestCompletionItem" maxOccurs="999">
				<xsd:annotation>
					<xsd:documentation>Position SumA</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
		<!-- Beendigung SumA -->
	</xsd:complexType>
	<!-- End new Entries for NCTS -->
	<xsd:complexType name="CT_ACPConnection">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type ACPConnection</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="NumberOfPackages" type="xsd:integer"/>
			<xsd:element name="SendToACP" type="xsd:string"/>
			<xsd:element name="SendToACPServer" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_AddInfoStatement">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">New in V 1.01 Complex Type Additional Information Statement</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Code" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Text" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Address">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Adress</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Adresse</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Street" type="xsd:string" minOccurs="0"/>
			<xsd:element name="HouseNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="City" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PostalCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Country" type="ST_CountryISOCode" minOccurs="0"/>
			<xsd:element name="CountrySubEntity" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_AddressType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="VATNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="ETNAddress" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Address" type="CT_Address" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ApprovedTreatment">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Customs Approved Treatment</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Verfahren(scode)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Declared" type="xsd:string"/>
			<xsd:element name="Previous" type="xsd:string"/>
			<xsd:element name="Current" type="xsd:string" minOccurs="0"/>
			<xsd:element name="National" type="xsd:string" minOccurs="0"/>
			<xsd:element name="National2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>New in V 1.01</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="National3" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>New in V 1.01</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CodeForRefund" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Community" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>New in V 1.01</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Community2" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>New in V 1.01</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Community3" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>New in V 1.01</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Business">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Business</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Geschäftsvorgang</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BusinessTypeCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation xml:lang="en"/>
					<xsd:documentation xml:lang="de">Art</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InvoicePrice" type="xsd:decimal"/>
			<xsd:element name="Currency" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Cancellation">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Cancellation</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Stornierung</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="KindOfCancellation" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Reason" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_CAP">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">New in V 1.01 Complex Type CAP Information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IBClaimRef" type="xsd:string" minOccurs="0"/>
			<xsd:element name="IBClaimType" type="xsd:string" minOccurs="0"/>
			<xsd:element name="IBRegNo" type="xsd:string" minOccurs="0"/>
			<xsd:element name="IBGAN" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_CommodityCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Commodity Code</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Warentarifnummer</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TarifCode" type="xsd:string"/>
			<xsd:element name="EUTarifCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="TarifAddition1" type="xsd:string" minOccurs="0"/>
			<xsd:element name="TarifAddition2" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Addition" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Addition2" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>New in V 1.01</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Addition3" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>New in V 1.01</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Completion">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Checked" type="ST_ConfirmationCode" minOccurs="0"/>
			<xsd:element name="TotalNumberOfPositions" type="xsd:int" minOccurs="0"/>
			<xsd:element name="AuthorizationNumber" type="xsd:string"/>
			<xsd:element name="ReferenceNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CompletionItem" type="CT_CompletionItem" maxOccurs="999"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_CompletionItem">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PositionNumber" type="xsd:integer"/>
			<xsd:element name="UCR" type="xsd:string"/>
			<xsd:element name="EntryInAtlas" type="ST_ConfirmationCode" minOccurs="0"/>
			<xsd:element name="Text" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Tarifnumber" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="UsualFormOfHandling" type="ST_ConfirmationCode" minOccurs="0"/>
			<xsd:element name="WriteDownAmount" type="CT_WriteDownAmount" minOccurs="0"/>
			<xsd:element name="TradeAmount" type="CT_WriteDownAmount" minOccurs="0"/>
			<xsd:element name="AtlasAlignment" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ManifestCompletionItem">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PositionNumber" type="xsd:integer"/>
			<xsd:element name="UCR" type="xsd:string"/>
			<xsd:element name="NumberOfPieces" minOccurs="0"/>
			<xsd:element name="AWB" type="xsd:string" minOccurs="0"/>
			<xsd:element name="CustodianTIN" type="CT_TINType" minOccurs="0"/>
			<xsd:element name="SpoArt" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Spo" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AtlasAlignment" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Container">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Container (only numbers)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Number" type="xsd:string" maxOccurs="99"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ContainerInformation">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Container (Number and  Movementinformation)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Container" type="CT_ContainerDetails" maxOccurs="99"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ContainerDetails">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type detailed information to Container </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Number" type="xsd:string"/>
			<xsd:element name="TransportMeans" type="CT_TransportMeansType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ContactPerson">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Position" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Clerk" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name of Clerk</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Identity" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>New in V 1.1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Email" type="xsd:string" minOccurs="0"/>
			<xsd:element name="FaxNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="PhoneNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Initials" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Title" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_CollectiveTerm">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Collective Term</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Sammelbegriff (ASR)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TermNumber" type="xsd:string"/>
			<!--			<xsd:element name="GoodsDeclaration" type="MT_ExitNotification" maxOccurs="unbounded"/> -->
		</xsd:sequence>
	</xsd:complexType>
