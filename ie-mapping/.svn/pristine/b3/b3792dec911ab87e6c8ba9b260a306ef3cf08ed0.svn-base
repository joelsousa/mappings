<?xml version="1.0" encoding="UTF-8"?>
<!-- Mit XMLSpy v2011 sp1 (http://www.altova.com) von XMLSpy 2011 Professional Ed., Concurrent for 5 users (Kewill GmbH) bearbeitet -->
<!-- 	Kewill Customs Exchange
		Kids Export Release 2.1
		Version 1.0 February 2012 

Changes:
February 2012: Introduced (KCXSM-18)		
August   2012: Jira ticket KCXSM-31 Christine Kron
April    2013: CT_ApprovedTreatment change types from int to string of: Declared, Previous, Current
August   2013: CT_StatisticType: new Member: StatisticalValueSendFlag 
	       CT_ContactPerson: new Member Name additional to Clerk. Both will be mapped to Name ,
	       deleted: TimeOfDecisionOfCancellation	
November 2013 changed tag type of TarifCode from integer to string	       
-->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<!-- complex types -->
	<xsd:complexType name="CT_ACPConnection">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type ACPConnection</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="NumberOfPackages" type="xsd:integer"/>
			<xsd:element name="SendToACP" type="xsd:string"/>
			<xsd:element name="SendToACPServer" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_AddInfoStatement">
		<xsd:sequence>
			<xsd:element name="Code" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Text" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_AddressType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="VATNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>VAT number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ETNAdress" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>ETN Address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Address" type="CT_Address" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Address">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Adress</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Adresse</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Street" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Street</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="HouseNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>House number (new in V 1.1)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="City" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>City</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PostalCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Postal Code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Country" type="ST_CountryISOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Country code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CountrySubEntity" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Subentity</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Amendment">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Amendment</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="KindOfAmendment" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Kind of amendment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Reason" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ApprovedTreatment">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Customs Approved Treatment</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Verfahren(scode)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Declared" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Declared customs approved treatment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Previous" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Previous customs approved treatment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Current" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>This section indicates if there is a current declaration procedure  which is relevant to the present item</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="National" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>National addition to the customs approved treatment</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="National2" minOccurs="0"/>
			<xsd:element name="National3" minOccurs="0"/>
			<xsd:element name="CodeForRefund" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>National code for export refund</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Community" minOccurs="0"/>
			<xsd:element name="Community2" minOccurs="0"/>
			<xsd:element name="Community3" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Business">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Business</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Geschäftsvorgang</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BusinessTypeCode" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Code of nature of the transaction (e.g. sale)</xsd:documentation>
					<xsd:documentation xml:lang="de">Art</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InvoicePrice" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Invoice price of the declaration</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Currency" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Currency of the invoice</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Cancellation">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Cancellation</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Stornierung</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="KindOfCancellation" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Kind of cancellation</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Reason" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_CAP">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type CAP Information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IBClaimRef" type="xsd:string" minOccurs="0"/>
			<xsd:element name="IBClaimType" type="xsd:string" minOccurs="0"/>
			<xsd:element name="IBRegNo" type="xsd:string" minOccurs="0"/>
			<xsd:element name="IBGAN" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_CollectiveTerm">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Collective Term</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Sammelbegriff (ASR)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TermNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Collective term like AWB, Containernumber or Harborreference</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_CommodityCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Commodity Code</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Warentarifnummer</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TarifCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>HS commoditycode number (first 8 digits)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EUTarifCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Addition to the commoditycode number, EU-Specification</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TarifAddition1" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Commodity code number supplement 1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TarifAddition2" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Commodity code number supplement 2</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Addition" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>national addition to the commodity code number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Addition2" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Addition3" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Confirmation" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Confirmatin of Commodity Code data used in CH</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Completion">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Checked" type="ST_ConfirmationCode" minOccurs="0"/>
			<xsd:element name="TotalNumberOfPositions" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Total number of positions</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AuthorizationNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Authorisation number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReferenceNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Reference number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CompletionItem" type="CT_CompletionItem" maxOccurs="999"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_CompletionItem">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SequentialNumber" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Sequential number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PositionNumber" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Position number  in the (bonded warehouse/inward processing) declaration</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UCR" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>UCR of the (bonded warehouse/inward processing)  declaration</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EntryInAtlas" type="ST_ConfirmationCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag if the inward processing declaration was made in atlas</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Text" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Text to describe the goods</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Tarifnumber" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Commodity code number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UsualFormOfHandling" type="ST_ConfirmationCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Usual form of handling the goods within the warehouse</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WriteDownAmount" type="CT_WriteDownAmount" minOccurs="0"/>
			<xsd:element name="TradeAmount" type="CT_WriteDownAmount" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ContactPerson">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Position" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Position of the contact person</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Clerk" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Name of Clerk</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Name" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Name instead of Clerk</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Identity" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>clerk identity in the application(new in V 1.1)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Email" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Email address of the person</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="FaxNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Fax number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PhoneNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Phone number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Initials" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Title" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Container">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Container (only numbers)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Number" type="xsd:string" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Number of container</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ContainerInformation">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Container (Number and  Movementinformation)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Container" type="CT_ContainerDetails" maxOccurs="99"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ContainerDetails">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type detailed information to Container </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Number" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Number of container</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransportMeans" type="CT_TransportMeansType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Means of transportation</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Detail">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Additional information to goods item</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Value" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_DocumentType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Produced Documents</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Unterlagen</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Qualifier" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Qualifier, addition to the code of the document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Type" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Code for the document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Reference" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Number for AEO or Authorisation number for outward processing or licence of the BAFA (Federal Office of Economics and Export Controls)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalInformation" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Additional information to the document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Detail" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Details to a document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IssueDate" type="ST_Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date the document was issued</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExpirationDate" type="ST_Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date the document will expire</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Value" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Amount to write down the export licence (in Euros without decimal places)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Reason" type="xsd:string" minOccurs="0"/>
			<xsd:element name="WritedownAmount" type="CT_WriteDownAmount" minOccurs="0"/>
			<xsd:element name="WritedownQuantity" type="CT_WriteDownAmount" minOccurs="0"/>
			<xsd:element name="Office" minOccurs="0"/>
			<xsd:element name="Present" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of presentation e.g. electronical or paper mould (new in V 1.01)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PresentLocation" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ErrorType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Fehler</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="UniqueNumber" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Unique number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Code" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Number and kind of the error (Warnung, Error, Information)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Text" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of the error</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Pointer" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Pointer to the part of the message with the error</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Number" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of the error (for CH)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ExportRefundHeader">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Export refund in header</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Ausfuhrerstattung Kopf</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Text" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Text for explanation</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PaymentType" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code how to pay the export refund</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BankNumber" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Sequential number for the bank account that has to be used</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Assignee" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Sequential number for the zustellungsbevollmächtigten</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Guarantee" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Sequential number for the guarantee that has to be used</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReservationCode" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>If this flag is used, than the declaration has be "described" otherwise you will not receive a "bescheid"</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DestinationCountry" type="ST_CountryISOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Country of destination</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_ExportRefundItem">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Export Refund in Item </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Ausfuhrerstattung in der Position</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Addition1" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Addition to the tarif description 1</xsd:documentation>
					<xsd:documentation xml:lang="de">Warenbezeichnung 1. Ergänzung</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Addition2" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Addition to the tarif description 2</xsd:documentation>
					<xsd:documentation xml:lang="de">Warenbezeichnung 2. Ergänzung</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OriginCountry" type="ST_CountryISOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Country of origin</xsd:documentation>
					<xsd:documentation xml:lang="de">Ursprungsland</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AmountCode" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Flag for estimate of the value in ExportRefundItem/Amount</xsd:documentation>
					<xsd:documentation xml:lang="de">Schätzwert</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Amount, which will be used to calculate the export refund</xsd:documentation>
					<xsd:documentation xml:lang="de">Menge</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TypeOfRefund" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Code for specifc types of refund</xsd:documentation>
					<xsd:documentation xml:lang="de">Zahlungsfall NA I - Erklärung des Ausführes hinsichtlich der "Nicht-Anhang I"-Budgetüberwachung. Die Inanspruchnahme der "Kleinausführer"-Regelung wird ebenso hier erklärt (Artikel 47 VO-EG 1043/2005) (Code)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AmountCoefficient" type="ST_Coefficient" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Coefficient to calculate the amount to be refunded</xsd:documentation>
					<xsd:documentation xml:lang="de">Anpassungskoeffizient </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PartA" type="ST_PartValue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">A value that has to be send for specific elements of the goods, only allowed for specific goods</xsd:documentation>
					<xsd:documentation xml:lang="de">Anteilswert A</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PartB" type="ST_PartValue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A value that has to be send for specific elements of the goods, only allowed for specific goods</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PartC" type="ST_PartValue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A value that has to be send for specific elements of the goods, only allowed for specific goods</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PartD" type="ST_PartValue" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>A value that has to be send for specific elements of the goods, only allowed for specific goods</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UnitOfMeasurement" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Unit of measurement</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ApplyFor" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Flag for: if reimbursement is applied for or not.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InAdvance" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Flag for:  if an advance reimbursement has been applied for.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CBT" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Flag for: Please indicate if reimbursement of CBT has been applied for or not.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Prefinancing" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The prefinancing code allocated by the commodity board.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ApplicationForm" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The five-digit code of the commodity board's authorisation to verification.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ApplicationDate" type="ST_Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The date when the application form was issued.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SimplifiedDeclaration" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The authorisation reference of the simplified export declaration.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SimplifiedDeclarationDate" type="ST_Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The date when authorisation to a simplified declaration was given</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Ingredient" type="CT_Ingredients" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Ingredients (commodity or preliminary products) for specific goods</xsd:documentation>
					<xsd:documentation xml:lang="de">Inhaltsstoff</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_GoodsIdentification">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type GoodsIdentification</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AdditionalCode" type="xsd:string" minOccurs="0" maxOccurs="9">
				<xsd:annotation>
					<xsd:documentation>Additional codes</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Grouping">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Grouping</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Vorgangszusammenfassung (ASL)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="GroupingReferenceNumber"/>
			<xsd:element name="CollectiveTerm"/>
			<xsd:element name="TypeOfTerm"/>
			<xsd:element name="TypeOfReference"/>
			<xsd:element name="Clerk"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_IncoTerm">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type IncoTerm</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Lieferbedingung</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="IncoTerm" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Incoterm</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Text" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Text for explanation</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Place" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Place for the incoterm</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LocationCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Specification of the Place for the incoterm</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Information">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Type" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Text" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Ingredients">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Ingredients Updated  in V 1.01</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Inhaltsstoff</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SequentialNumber" type="xsd:integer"/>
			<xsd:element name="ApplyFor" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Flag for: if reimbursement is applied for or not.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount1" type="ST_Coefficient" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Coefficient to adapt the refund amount</xsd:documentation>
					<xsd:documentation xml:lang="de">Umrechnungsfaktor 1</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="KindOfCoefficient" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag for Ingredients/Amount1, to identify if this coefficient has to be used to multiply or to divide</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Measure1" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Type of measure used to calculated the amount of reimbursement.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Amount2" type="ST_Coefficient" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Coefficient to adapt the refund amount</xsd:documentation>
					<xsd:documentation xml:lang="de">Umrechnungsfaktor 2</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Rate" type="ST_Coefficient" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Coefficient to adapt the refund rate</xsd:documentation>
					<xsd:documentation xml:lang="de">Umrechnungsfaktor-Satz</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Measure2" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Type of measure used to calculated the amount of reimbursement.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WeightPercent" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Percent by weight</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Weight" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Weight of the ingredient in kilogram</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UniqueFactoryNumber" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>By sending the uniquefactory number the refund of the recipe of the preliminary products is described</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TarifNumber" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Commodity code, first 6 digits are hs6, the last 4 digits are contained in VF M 8012</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LicenceNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Licence number for the ingredient</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="LicenceLineNumber" minOccurs="0"/>
			<xsd:element name="LicenceStatus" minOccurs="0"/>
			<xsd:element name="LicenceQuantity" minOccurs="0"/>
			<xsd:element name="Text" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Description of the ingredient</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="BeneficiaryTIN" type="CT_TINType"/>
			<xsd:element name="Beneficiary" type="CT_AddressType" minOccurs="0"/>
			<xsd:element name="SupplierTIN" type="CT_TINType"/>
			<xsd:element name="Supplier" type="CT_AddressType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Invoice">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Invoice (NL)</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="InvoiceNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Invoice number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InvoicePrice" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Invoice price</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Currency" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Currency of the invoice price</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IncoTerms" type="CT_IncoTerm" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>IncoTerm</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ConsigneeTIN" type="CT_TINType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>TIN data of conisgnne</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Consignee" type="CT_AddressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Conisgnne</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ConsignorTIN" type="CT_TINType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>TIN data of conisgnor</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Consignor" type="CT_AddressType" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Conisgnor</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InvoiceItem" type="CT_InvoiceItem" minOccurs="0" maxOccurs="99999"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_InvoiceItem">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type InvoiceItem</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ItemNumber" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Number of the item</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Description of the goods</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NetMass" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Net mass</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GrossMass" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Gross mass</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="InvoicePrice" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Invoice line price</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Currency" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Currency of the invoice line price</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Packages" type="CT_InvoicePackage" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Packaging</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_InvoicePackage">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type InvoicePackage</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Package" type="CT_InvoicePackageType" maxOccurs="5">
				<xsd:annotation>
					<xsd:documentation>Packaging</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_InvoicePackageType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type InvoicePackage</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Quantity" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Quantity</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Type" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Type of packiging (Code for pallets, boxes,...)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Marks" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Marks and numbers</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_InspectionCode">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type InspectionCode</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="InspectionCode" type="xsd:string" minOccurs="0" maxOccurs="9"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_LoadingTime">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Loading Time</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Gestellung/Verpackung/Verladen</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="BeginTime" type="ST_DateTime">
				<xsd:annotation>
					<xsd:documentation>Begin time of loading</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndTime" type="ST_DateTime">
				<xsd:annotation>
					<xsd:documentation>End time of loading</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_MeansOfIdentification">
		<xsd:sequence>
			<xsd:element name="Type" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Text" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Movement">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type MultipleMessages</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Sammelqualifizierung oder Sammelumfuhr</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TypeOfMessage"/>
			<xsd:element ref="ReferenceNumber"/>
			<xsd:element ref="KindOfInformation"/>
			<xsd:element ref="FlagInformation"/>
			<xsd:element ref="MeansOfTransport"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_NonCustomsLawType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Non Customs Law </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type NZE-Art</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Obligation"/>
			<xsd:element name="Type" type="xsd:string" minOccurs="0" maxOccurs="9">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Flag, depends on the used commodity code, if a check has to be or can be made, of if no check is necessary</xsd:documentation>
					<xsd:documentation xml:lang="de">NZE-Arten Code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_NotificationCodeType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Notification Code </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Meldungen</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="NotificationCode" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Notification code </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_OutwardProcessing">
		<xsd:sequence>
			<xsd:element name="AuthorizationNumber" type="xsd:string" minOccurs="0"/>
			<xsd:element name="AuthorizationLocalClearenceProcedure" type="xsd:string" minOccurs="0"/>
			<xsd:element name="DateOfReExport" type="ST_DateTime" minOccurs="0"/>
			<xsd:element name="StandardExchange" type="xsd:int" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_PackagingType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Packaging Type</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Packstücke</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Quantity" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Quantity</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SequentialNumber" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>Number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Type" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of packiging (Code for pallets, boxes,...)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Marks" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Marks and numbers</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransportMeans" type="CT_TransportMeansType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_PDFInformation">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex PDF Information</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Name" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Name of the pdf-document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Directory" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Path, where to find the pdf document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="NewDirectory" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>directory, which contains the pdf document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Remark" minOccurs="0"/>
			<xsd:element name="base64" type="xsd:base64Binary" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Encoded text of the pdf documen</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_PermitType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="PermitAuthority" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code for the authority, where the permit comes from</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PermitNumber" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Number of the permit</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IssueDate" type="ST_Date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Date of the permit</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalInformation" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Additional information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Type" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_PlaceOfLoading">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type place of loading</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Warenort/Ladeort</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Code" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code for the place of loading</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Street" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Street and Number for the place of loading</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PostalCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Postal code for the place of loading</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="City" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>City for the place of loading</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AgreedLocationOfGoods" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Additional information to the place of loading</xsd:documentation>
					<xsd:documentation xml:lang="de">Zusatz/bewilligter Ladeort</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_PositionError">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Position Error</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Fehler pro Position</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<!-- xsd:element name="KindOfError" type="xsd:int"-->
			<xsd:element name="KindOfError" type="ST_KindOfError">
				<xsd:annotation>
					<xsd:documentation>Indicator if PositionError list warnings (WRG), information (INF) or errors (ERR)</xsd:documentation>
					<xsd:documentation>the original message referred to is not sent to customs because of errors if there is at least one appearance of the code "ERR" in the tag ErrorCode
INF / WRG can be used in case of "confirm" and also in case of "failure" 

</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="OriginOfError" type="xsd:int" minOccurs="0"/>
			<xsd:element name="Modul" type="xsd:int" minOccurs="0"/>
			<xsd:element name="ErrorCode" type="xsd:string"/>
			<xsd:element name="ErrorText" type="xsd:string" minOccurs="0"/>
			<xsd:element name="DateOfErrorMessage" type="ST_Date" minOccurs="0"/>
			<xsd:element name="TimeOfErrorMessage" type="ST_Time" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_PreviousDocumentType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type previous document</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type summarische Anmeldung vorpapier</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Type" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code for type of previous document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="DocumentType" type="xsd:string" minOccurs="0"/>
			<xsd:element name="Marks" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Marks and Number</xsd:documentation>
					<xsd:documentation xml:lang="de">Zeichen (Nummer) Vorpapier/Referenz</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Reference" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Document Reference/Number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ReferenceSpecification" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="Quantity" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="GrossMass" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="NetMass" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="UnitOfMeasurement" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="Value" type="xsd:decimal" minOccurs="0"/>
			<xsd:element name="AdditionalInformation" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Additional information to describe the previous document</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Date" minOccurs="0"/>
			<xsd:element name="Office" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Product">
		<xsd:sequence>
			<xsd:element name="TarifCode" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>HS commoditycode number (first 8 digits)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Description" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Reentry">
		<xsd:sequence>
			<xsd:element name="Country" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Refinement">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Refinement</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Direction" type="xsd:string" minOccurs="0"/>
			<xsd:element name="RefinementType" minOccurs="0"/>
			<xsd:element name="ProcessType" minOccurs="0"/>
			<xsd:element name="BillingType" minOccurs="0"/>
			<xsd:element name="TemporaryAdmission" minOccurs="0"/>
			<xsd:element name="PositionType" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Route">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type route</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Beförderungsroute</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Country" type="ST_CountryISOCode" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Country the shipment will cross the border</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Seal">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type seals</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Verschlüsse</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Kind" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Kind of seal</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Number" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Total number of used seals</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UseOfTydenseals" type="ST_ConfirmationCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag if tydenseals will be used</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UseOfTydensealStock" type="ST_ConfirmationCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag if the tydenseals will be taken from the tyden seal stock</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SealNumbers" type="CT_SealNumbers" minOccurs="0" maxOccurs="99">
				<xsd:annotation>
					<xsd:documentation>Number of the used seal</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--  added 2012-03-01 Atlas 8.4 Start-->
			<xsd:element name="SealOK" type="xsd:int" minOccurs="0"/>
			<!--  added 2012-03-01 Atlas 8.4 End-->
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_SealNumbers">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type seal</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Verschluss</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Number" type="xsd:string">
				<xsd:annotation>
					<xsd:documentation>Number of the used seal</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SealingParty" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_SensitiveGoodsType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Dangerous Goods</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Empfindliche Waren</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Type" type="xsd:integer">
				<xsd:annotation>
					<xsd:documentation>CommodityCode for senstive goods</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Weight" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Weight for the sensitive goods</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_SpecialMentionType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TypeOfExport" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag if the goods in the item do have any restrictions for export</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExportFromEU" type="ST_ConfirmationCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Flag if the text of export depends on an export starting from the EU</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ExportFromCountry" type="ST_CountryISOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Export from a specific country</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Text" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Text</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element ref="Language" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_StatisticType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="AdditionalUnit" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Amount</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalUnitConfirmation" type="ST_ConfirmationCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Correction code for the amount</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AdditionalUnitCode" minOccurs="0"/>
			<xsd:element name="StatisticalValue" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value  </xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StatisticalValueConfirmation" type="ST_ConfirmationCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Correction code for the value</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="StatisticalValueSendFlag" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Values: 1, 0 or empty
1 = StatisticalValue equal null will be send to Customs,
0 or no Tag sent = StatisticalValue equal null will not be send to Customs,</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:length value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="Value" type="xsd:decimal" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Value to calculate the value from another currency</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Currency" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Currency to statistic/value</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_TINType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TIN" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>TIN</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="IsTINGermanApprovalNumber" type="ST_ConfirmationCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Is TIN a german approval number</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CustomerIdentifier" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Customer identidier</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--  added 2012-03-01 Atlas 8.4 Start-->
			<xsd:element name="BO" type="xsd:int" minOccurs="0"/>
			<xsd:element name="IdentificationType" type="xsd:int" minOccurs="0"/>
			<!--  added 2012-03-01 Atlas 8.4 End -->
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_TransportMeansType">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type TransportMeansType</xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type Beförderungsmittel</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="TransportMode" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Mode of transport</xsd:documentation>
					<xsd:documentation xml:lang="de">Verkehrszweig</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransportationType" type="xsd:integer" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Type of the means of transport</xsd:documentation>
					<xsd:documentation xml:lang="de">Art</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransportTime" type="ST_DateTime" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Time of transportation</xsd:documentation>
					<xsd:documentation xml:lang="de">Zeitpunkt der Abfahrt bzw. des Abflugs</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransportationNumber" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Licence plate number of the means of transport</xsd:documentation>
					<xsd:documentation xml:lang="de">Kennzeichen</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransportationCountry" type="ST_CountryISOCode" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Nationality of the means of transport</xsd:documentation>
					<xsd:documentation xml:lang="de">Staatszugehörigkeit</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PlaceOfLoading" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Place of loading of the means of transport</xsd:documentation>
					<xsd:documentation xml:lang="de">Ladeort</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="PlaceOfLoadingCode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Code for place of loading</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_Warehouse">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type Warehouse</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="WarehouseType" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Type of the warehouse</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WarehouseIdentification" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Identification of the warehouse</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="WarehouseCountry" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>The country of the warehouse</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="CT_WriteDownAmount">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Complex Type </xsd:documentation>
			<xsd:documentation xml:lang="de">Complex Type </xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="Qualifier" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Qualifier for the unit of measurement</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="UnitOfMeasurement" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Unit of measurement</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Value" type="xsd:decimal">
				<xsd:annotation>
					<xsd:documentation>Value to write down the export licence</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ###############################################################  -->
	<!-- simple types -->
	<xsd:simpleType name="ST_CustomsOffice">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="8"/>
			<xsd:maxLength value="8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_Currency">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="3"/>
			<xsd:maxLength value="3"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_ConfirmationCode">
		<xsd:restriction base="xsd:boolean"/>
	</xsd:simpleType>
	<xsd:simpleType name="ST_CountryISOCode">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="2"/>
			<xsd:maxLength value="2"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_Date">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="8"/>
			<xsd:maxLength value="8"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_DateTime">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="12"/>
			<xsd:maxLength value="12"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_DateFormatted">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{4}[\-][0-9]{2}[\-][0-9]{2}">
				<xsd:annotation>
					<xsd:documentation>
						format:		YYYY-MM-DD 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_DateTimeFormatted">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{4}[\-][0-9]{2}[\-][0-9]{2}(T)[0-9]{2}[:][0-9]{2}[:][0-9]{2}([+\-][0-9]{2}[:][0-9]{2}|(Z))">
				<xsd:annotation>
					<xsd:documentation>
						format:		YYYY-MM-DDTHH:MM:SS(+HH:MM|Z)
					</xsd:documentation>
				</xsd:annotation>
			</xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_Coefficient">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<xsd:simpleType name="ST_PartValue">
		<xsd:restriction base="xsd:decimal"/>
	</xsd:simpleType>
	<xsd:simpleType name="ST_TimeFormatted">
		<xsd:restriction base="xsd:string">
			<xsd:pattern value="[0-9]{2}[:][0-9]{2}([:][0-9]{2}([.][0-9]{3}))">
				<xsd:annotation>
					<xsd:documentation>
						format:		HH:MM[:SS[.sss]] 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:pattern>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_Time">
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="6"/>
			<xsd:maxLength value="6"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="ST_KindOfError">
		<xsd:restriction base="xsd:token">
			<xsd:enumeration value="ERR">
				<xsd:annotation>
					<xsd:documentation>Error</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="WRG">
				<xsd:annotation>
					<xsd:documentation>Warning</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="INF">
				<xsd:annotation>
					<xsd:documentation>Information</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- ###############################################################  -->
	<!-- elements -->
	<xsd:element name="AreaCode" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Type of Declaration (Export)</xsd:documentation>
			<xsd:documentation xml:lang="de">Art der Anmeldung (Ausfuhr)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TypeOfPermit" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Type of Declaration (kind of permit)</xsd:documentation>
			<xsd:documentation xml:lang="de">Art der Anmeldung (Überführung)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="DeclarationKind" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation>Flag if the message will also be an NCTS-Declaration or not</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="KindOfDeclaration" type="xsd:string">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Kind of Declaration CH (first Step or second Step)</xsd:documentation>
			<xsd:documentation xml:lang="de">Art der Deklaration</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="TypeOfPermitCH" type="xsd:int">
		<xsd:annotation>
			<xsd:documentation xml:lang="en">Status of Declaration that informs if it is a one or two-step procedure</xsd:documentation>
			<xsd:documentation xml:lang="de">Statuscode (CH)</xsd:documentation>
		</xsd:annotation>
	</xsd:element>
	<xsd:element name="KindOfAnswer" type="xsd:integer">
