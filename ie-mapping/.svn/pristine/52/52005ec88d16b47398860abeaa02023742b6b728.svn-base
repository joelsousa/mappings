<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:include schemaLocation="kids_general_types.xsd"/>
	<xsd:element name="InternalStatusInformation">
		<xsd:annotation>
			<xsd:documentation>Internal Status Information</xsd:documentation>
			<xsd:documentation xml:lang="de">Statusmeldung</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="GoodsDeclaration">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="ReferenceNumber"/>
							<xsd:element ref="UCRNumber" minOccurs="0"/>
							<xsd:element ref="OrderNumber" minOccurs="0"/>
							<xsd:element ref="CorrectionNumber" minOccurs="0"/>
							<xsd:element ref="TemporaryUCR" minOccurs="0"/>
							<xsd:element ref="DateNewStatus" minOccurs="0"/>
							<xsd:element ref="TimeNewStatus" minOccurs="0"/>
							<xsd:element ref="NewStatus" minOccurs="0"/>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
