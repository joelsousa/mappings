	<xsd:element name="GoodsDeclaration">
	<xsd:annotation>
		<xsd:documentation>Goods Declaration</xsd:documentation>
	</xsd:annotation>				
	<xsd:complexType>		       
	<xsd:sequence>
		
				<xsd:element name="ReferenceNumber" minOccurs="0">
